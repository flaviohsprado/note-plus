{
  "expo": {
    "name": "note-plus",
    "slug": "note-plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Rubik-Black.ttf", "./assets/fonts/Rubik-Bold.ttf", "./assets/fonts/Rubik-Light.ttf", "./assets/fonts/Rubik-Medium.ttf", "./assets/fonts/Rubik-Regular.ttf", "./assets/fonts/Rubik-SemiBold.ttf"]
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mashida.noteplus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0a34ed57-0e43-439e-9540-6062c6e99d0d"
      }
    },
    "owner": "mashida"
  }
}
